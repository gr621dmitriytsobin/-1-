import 'dart:io';
void main()
{
print("Ввести координату a");
int a = int.parse(stdin.readLineSync()!);
print("Ввести координату b");
int b = int.parse(stdin.readLineSync()!);
if ((-1 < a) && (a < 3) && (-2 < b) && (b < 4)) 
{
  print("Точка с координатами a и b - принадлежит заштрихованной области");
}
else if (( a == -1 ) && (-2 < b) && (b < 4) || ((a == 3) && (-2 < b) && (b < 4))) 
{
print("Точка скоординатами a и b находится на границе области  ");
}
else if ((a == -1) && (b == -2) || (a == 3) && (b == -2) || (a == 3) && (b == 4))
{
  print("Точка скоординатами a и b находится в углу на границе  области ");
}
else 
{
  print("Точка скоординатами a и b не принадлежит заштрихованной области");
}
}


