// 1 задание 

 import 'dart:io';
void main()
{
print("Ввести целое положительное число n");
int n = int.parse(stdin.readLineSync()!);
int work = 1;
for (int i = 3; i <= n; i & 3 ==0) {
work *= i;
}
print(work);
}


