import 'dart:io';
import 'dart:math';

void main() {
 Random random = new Random();
 List<double> oneList = [];
 for (int i = 0; i < 20; i++) {
 oneList.add((random.nextInt(100)- 50).toDouble());
}
List<double> positiveNums = [];
List<double> negativeNums = [];
for (int i = 0; i < oneList.length; i++) {
 if (oneList[i] >= 0) {
   positiveNums.add(oneList[i]);
  } else {
negativeNums.add(oneList[i]);
 }
}
print("Positive - $positiveNums");
print("Negative - $negativeNums");
}
