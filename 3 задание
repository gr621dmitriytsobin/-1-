import "dart:io"; 
import "dart:math"; 

void main(List<String> args) 
{ 
int  largeValue = 1; 
int smallestValue = 1;  
var nums = [1]; 
print(nums); 
for (int i = 0; i < nums.length; i++)
{ 
print("Добавьте новые элементы в список: "); 
int i = int.parse(stdin.readLineSync()!); 
smallestValue = i; 
if ( i != 0)
{ 
largeValue += i; 
nums.add(i); 
print(nums); 
} 
else 
{ 
print(" "); 
break; 
} 
print("Самый короткий элемент списка - $smallestValue"); 
print("Самый длинный элемент списка - $largeValue");  
} 
